    "def isPrime(n):\n",
    "    i=2\n",
    "    while i<=n-1:\n",
    "        if n%i==0:\n",
    "            return 0\n",
    "        else: i=i+1\n",
    "    return 1\n",
    "a=input(\"请输入下限\")\n",
    "b=input(\"请输入上限\")\n",
    "for i in range(int(a),int(b)):\n",
    "    if isPrime(i):\n",
    "        print(i)"
